{
    "manifest_version": 2,
    "name": "UnHighlight",
    "version": "0.2",
    "description": "Strips ?highlight=... from URLs",
    "author": "Justin Bassett",
    "homepage_url": "https://github.com/Quincunx271/UnHighlight",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {"scripts": ["unhighlight.js"]},
    "icons": {
        "48": "build/logo48x48.png",
        "96": "build/logo96x96.png",
        "128": "build/logo128x128.png"
    }
}
